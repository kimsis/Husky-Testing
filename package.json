  "dependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1"
  }
}
