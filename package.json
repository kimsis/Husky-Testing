{
  "name": "my-husky-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no -- commitlint --edit \"${1}\""
